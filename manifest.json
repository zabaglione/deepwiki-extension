{
    "manifest_version": 3,
    "name": "DeepWiki Toggle",
    "version": "1.1",
    "description": "GitHubリポジトリとDeepWikiをワンクリックで切り替える",
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}